.StoreDetailNull {
  margin: 0 auto;
  width: 510rpx;
  height: 800rpx;
  border-radius: 40rpx;
  background: rgba(128, 129, 145, 0.04);
  box-shadow: 8rpx 20rpx 40rpx 0rpx rgba(0, 0, 0, 0.06);
}
.StoreDetailNull .StoreDetailI {
  margin-top: 130rpx;
  width: 510rpx;
  height: 400rpx;
  border-radius: 40rpx;
  background: linear-gradient(180deg, #cde7ff 0%, #c1e1ff 100%);
  display: flex;
  align-items: center;
  justify-content: center;
}
.StoreDetailNull .StoreDetailP {
  margin-top: 40rpx;
}
.StoreDetailNull .StoreDetailP text:nth-child(1) {
  display: block;
  height: 60rpx;
  font-size: 40rpx;
  font-weight: 700;
  line-height: 60rpx;
